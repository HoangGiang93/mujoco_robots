<launch>

    <arg name="namespace" default="zoo" />

    <group ns="$(arg namespace)">
        <include file="$(find mujoco_sim)/launch/mujoco_sim.launch">
            <arg name="config" value="$(find zoo_mujoco)/config/zoo.yaml" />
            <arg name="robot" value="$(find zoo_mujoco)/model/zoo.xml" />
            <arg name="world" value="$(find zoo_mujoco)/model/world.xml" />
        </include>
    </group>

</launch>