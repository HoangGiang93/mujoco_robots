<launch>

    <arg name="namespace" default="panda_arm"/>
    <arg name="config" default="$(find franka_mujoco)/config/dual_panda_arm.xml" />
    <arg name="pub_object_marker" default="true" />
    <arg name="pub_object_state" default="false" />

    <group ns="$(arg namespace)">
        <include file="$(find mujoco_sim)/launch/mujoco_sim.launch">
            <arg name="robot" value="$(find franka_mujoco)/model/dual_panda_arm.xml" />
            <arg name="config" value="$(arg config)" />
            <arg name="pub_object_marker" value="$(arg pub_object_marker)" />
            <arg name="pub_object_state" value="$(arg pub_object_state)" />
        </include>
    </group>

</launch>