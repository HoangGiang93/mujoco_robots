<launch>

    <arg name="namespace" default="panda_arm"/>
    
    <group ns="$(arg namespace)">
        <include file="$(find mujoco_sim)/launch/mujoco_sim.launch">
            <arg name="robot" value="$(find franka_mujoco)/model/panda_arm.xml" />
            <arg name="config" value="$(find franka_mujoco)/config/panda_arm.xml" />
        </include>
    </group>

</launch>