<launch>

	<!-- Load controller manager -->
	<rosparam command="load" file="$(find tiago_control)/config/default_controllers.yaml" />
	<node name="ros_control_controller_manager" pkg="controller_manager" type="controller_manager" respawn="false" output="screen" args="spawn 
		joint_state_controller
		torso_controller
		head_controller
		arm_left_controller
		arm_right_controller
		gripper_left_left_finger_effort_controller
		gripper_left_right_finger_effort_controller
		gripper_right_left_finger_effort_controller
		gripper_right_right_finger_effort_controller"/>

	<node name="rqt_joint_trajectory_controller" pkg="rqt_joint_trajectory_controller" type="rqt_joint_trajectory_controller" />

</launch>